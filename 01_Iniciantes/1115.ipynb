   "metadata": {},
   "outputs": [],
   "source": [
    "while True:\n",
    "\n",
    "    x, y = map(int, input().split())\n",
    "    if (x == 0) | (y == 0): break\n",
    "\n",
    "    if (x > 0) & (y > 0): print(\"primeiro\")\n",
    "    elif (x < 0) & (y > 0): print(\"segundo\")\n",
    "    elif (x < 0) & (y < 0): print(\"terceiro\")\n",
    "    else: print(\"quarto\")\n"
   ]
  }
