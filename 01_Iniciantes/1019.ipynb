   "id": "323824e4",
   "metadata": {},
   "outputs": [],
   "source": [
    "total_segundos = int(input())\n",
    "\n",
    "relogio = {3600 : 0, 60 : 0, 1: 0}\n",
    "for i in relogio.keys():\n",
    "    relogio[i] = total_segundos // i\n",
    "    total_segundos -= relogio[i] * i\n",
    "\n",
    "print(f\"{relogio[3600]}:{relogio[60]}:{relogio[1]}\")"
   ]
  }
