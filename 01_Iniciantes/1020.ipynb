    "### Problema 1020 - Idade em Dias"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a96cc383",
   "metadata": {},
   "outputs": [],
   "source": [
    "dias = int(input())\n",
    "idade = {365 : ['ano(s)', 0], 30 : ['mes(es)', 0], 1 : ['dia(s)', 0]}\n",
    "\n",
    "for i in idade.keys():\n",
    "    idade[i][1] = dias // i\n",
    "    dias -= idade[i][1] * i\n",
    "    print(f\"{idade[i][1]} {idade[i][0]}\")"
   ]
  }
